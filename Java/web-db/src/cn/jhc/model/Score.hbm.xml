<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.jhc.model">

    <class name="Score" table="Score">
        <composite-id class="ScorePK" name="pk">
    		<key-property name="sno"/>
    		<key-property name="cno"/>
    	</composite-id>
        <property name="uscore"/>
        <property name="endScore"/>
    </class>

</hibernate-mapping>
